{
  "appId": "com.boostinghost.app",
  "appName": "Boostinghost",
  "webDir": "public",
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": true,
      "backgroundColor": "#3B82F6",
      "androidSplashResourceName": "splash",
      "showSpinner": false
    },
    "StatusBar": {
      "style": "light",
      "backgroundColor": "#3B82F6",
      "overlaysWebView": false
    },
    "Haptics": {
      "enabled": true
    }
  }
}
